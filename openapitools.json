{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.6.0",
    "useDocker": true,
    "generators": {
      "python": {
        "output": "py_generated",
        "inputSpec": "common.yaml",
        "generatorName": "python",
        "additionalProperties": {
          "generateSourceCodeOnly": "true",
          "packageName": "bounan_common"
        }
      }
    }
  }
}
